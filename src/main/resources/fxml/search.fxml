<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="648.0" prefWidth="1152.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uoa.lavs.controllers.SearchController">
   <children>
      <Button fx:id="startButton" layoutX="74.0" layoutY="55.0" maxHeight="60.0" maxWidth="150.0" minHeight="60.0" minWidth="150.0" mnemonicParsing="false" onAction="#onClickStart" stylesheets="@../css/application.css" text="Back">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="searchField" layoutX="326.0" layoutY="55.0" onKeyPressed="#keyPressed" prefHeight="60.0" prefWidth="500.0" promptText="Search Customers">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="searchButton" layoutX="873.0" layoutY="55.0" mnemonicParsing="false" onAction="#onClickSearch" prefHeight="60.0" prefWidth="150.0" text="Search">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TableView fx:id="searchTable" fixedCellSize="50.0" layoutX="74.0" layoutY="150.0" prefHeight="450.0" prefWidth="1004.0">
        <columns>
          <TableColumn fx:id="idColumn" maxWidth="252.0" minWidth="252.0" prefWidth="-1.0" resizable="false" text="ID" />
          <TableColumn fx:id="nameColumn" maxWidth="500.0" minWidth="500.0" prefWidth="-1.0" text="Name" />
            <TableColumn fx:id="dobColumn" maxWidth="253.0" minWidth="253.0" prefWidth="-1.0" text="Date of Birth" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
