<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="648.0" prefWidth="1152.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uoa.lavs.controllers.SearchController">
   <children>
      <Label fx:id="searchSceneTitleLabel" alignment="CENTER" layoutX="376.0" layoutY="25.0" maxHeight="50.0" maxWidth="400.0" minHeight="50.0" minWidth="400.0" text="Customer Search">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="74.0" layoutY="85.0" maxHeight="60.0" maxWidth="150.0" minHeight="60.0" minWidth="150.0" mnemonicParsing="false" onAction="#onClickBack" stylesheets="@../css/application.css" text="Back">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="searchField" layoutX="326.0" layoutY="85.0" onKeyPressed="#keyPressed" prefHeight="60.0" prefWidth="500.0" promptText="Search Customers by Name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="searchButton" layoutX="928.0" layoutY="85.0" mnemonicParsing="false" onAction="#onClickSearch" prefHeight="60.0" prefWidth="150.0" text="Search">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TableView fx:id="searchTable" fixedCellSize="50.0" layoutX="74.0" layoutY="175.0" maxHeight="425.0" maxWidth="1004.0" minHeight="425.0" minWidth="1004.0">
        <columns>
          <TableColumn fx:id="idColumn" editable="false" maxWidth="252.0" minWidth="252.0" prefWidth="-1.0" resizable="false" sortable="false" text="ID" />
          <TableColumn fx:id="nameColumn" editable="false" maxWidth="500.0" minWidth="500.0" prefWidth="-1.0" resizable="false" sortable="false" text="Name" />
            <TableColumn fx:id="dobColumn" editable="false" maxWidth="253.0" minWidth="253.0" prefWidth="-1.0" resizable="false" sortable="false" text="Date of Birth" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
