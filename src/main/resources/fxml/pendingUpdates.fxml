<AnchorPane prefHeight="648.0" prefWidth="1152.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uoa.lavs.controllers.PendingUpdatesController">
   <children>
      <Button fx:id="backButton" layoutX="44.0" layoutY="30.0" maxHeight="60.0" maxWidth="150.0" minHeight="60.0" minWidth="150.0" mnemonicParsing="false" onAction="#onClickBack" stylesheets="@../css/application.css" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="sendButtton" layoutX="933.0" layoutY="32.0" mnemonicParsing="false" onAction="#onClickSend" prefHeight="60.0" prefWidth="150.0" text="Send">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="335.0" layoutY="77.0" maxHeight="50.0" maxWidth="484.0" minHeight="50.0" minWidth="400.0" prefHeight="50.0" prefWidth="484.0" text="Pending Mainframe Updates">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TableView fx:id="pendingTable" fixedCellSize="50.0" layoutX="74.0" layoutY="175.0" prefHeight="425.0" prefWidth="1004.0" stylesheets="@../css/application.css">
